<h1>New Prospect</h1>


<%= form_tag(prospects_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

  <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>


  <div>
    <%= submit_tag "Create Prospect" %>
  </div>
<% end %>
